<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jackl.mvparticle">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name="com.jackl.mvparticle.base.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".view.RootActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".view.live.impl.LivePlayActivity"></activity>

        <activity android:name=".view.example.example_one.ExampleOneActivity"></activity>

        <activity android:name=".view.example.example_one.ExampleOneOtherActivity"></activity>

        <activity android:name=".view.example.example_two.ExampleTwoActivity"></activity>

        <activity android:name=".view.example.example_three.impl.ExampleThreeActivity"></activity>

        <activity android:name=".view.example.example_four.impl.ExampleFourActivity"></activity>

        <activity android:name=".view.example.example_five.impl.ExampleFiveActivity"></activity>

    </application>

</manifest>